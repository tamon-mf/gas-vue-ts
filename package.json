{
	"name": "gas-test",
	"private": true,
	"version": "0.0.0",
	"type": "module",
	"scripts": {
		"dev": "vite",
		"dev:watch": "concurrently \"npm run build:client:watch\" \"npm run build:server:watch\" \"npm run push:watch\"",
		"preview": "vite preview",
		"build:client": "vite build",
		"build:client:watch": "vite build --watch",
		"old:build:server": "tsx scripts/build.ts",
		"old:build:server:watch": "WATCH=true tsx scripts/build.ts",
		"build:server": "rolldown -c rolldown.config.ts",
		"build:server:watch": "rolldown -c rolldown.config.ts -w",
		"build": "npm run build:client && npm run build:server",
		"build:watch": "concurrently \"npm run build:client:watch\" \"npm run build:server:watch\"",
		"push": "NODE_TLS_REJECT_UNAUTHORIZED=0 clasp push",
		"push:watch": "chokidar \"dist/**\" -c \"NODE_TLS_REJECT_UNAUTHORIZED=0 clasp push\" --initial"
	},
	"devDependencies": {
		"@biomejs/biome": "2.3.8",
		"@rollup/plugin-alias": "^6.0.0",
		"@types/google-apps-script": "^2.0.8",
		"@types/node": "^24.10.1",
		"@vitejs/plugin-vue2": "^2.3.4",
		"chokidar-cli": "^3.0.0",
		"concurrently": "^9.2.1",
		"esbuild": "^0.27.0",
		"path": "^0.12.7",
		"rolldown": "1.0.0-beta.52",
		"rolldown-plugin-remove-export": "^0.1.3",
		"tsx": "^4.21.0",
		"typescript": "~5.9.3",
		"vite": "^7.2.4",
		"vite-plugin-dts": "^4.5.4",
		"vite-plugin-singlefile": "^2.3.0",
		"vue-template-compiler": "2.6.14"
	},
	"dependencies": {
		"vue": "2.7.16",
		"vuetify": "2.7.2"
	}
}
